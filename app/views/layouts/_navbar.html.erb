<nav>
  <%=image_tag("turing_logo_wellness.png", size: '90x90')%>
  <% if current_user %>
  <a class="app-title" href= "<%=user_goals_path%>">Wellness Tracker</a>
  <% end %>

<% if current_user %>
  <details close>
    <summary><%=current_user.name%> </summary>
      <p><% if current_user.admin? %>
        <%= link_to "Admin Dashboard", admin_dashboard_index_path  %>
      <% end %></p>

      <p><%= link_to "Edit profile", edit_user_path(current_user) %></p>
      <p><% if current_user %>
        <%= link_to "Logout", logout_path, method: :delete %></p>
      <% end %>
  </details>
<% end %>
</nav>
